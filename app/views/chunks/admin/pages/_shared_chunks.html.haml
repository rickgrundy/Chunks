%ul.available_chunks
  -container.available_shared_chunks.each do |shared|
    %li{class: shared.chunk.class.partial_name}
      =link_to shared.name, include_shared_admin_chunk_path(id: shared.chunk, page_id: @page.id, container_key: container.key),
        style: "background-image: url('/assets/chunks/#{shared.chunk.class.partial_name}.png')"